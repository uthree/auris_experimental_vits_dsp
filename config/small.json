{
    "vits": {
        "segment_size": 32,
        "generator": {
            "decoder": {
                "content_channels": 96,
                "speaker_embedding_dim": 128,
                "sample_rate": 48000,
                "frame_size": 480,
                "num_harmonics": 0,
                "upsample_initial_channels": 256,
                "resblock_type": "2",
                "resblock_kernel_sizes": [3, 5, 7],
                "resblock_dilations": [[1, 2], [2, 6], [3, 12]],
                "upsample_kernel_sizes": [24, 20, 8],
                "upsample_rates": [12, 10, 4]
            },
            "pitch_estimator": {
                "content_channels": 96,
                "internal_channels": 256,
                "num_layers": 2,
                "num_classes": 512,
                "classes_per_octave": 48,
                "min_frequency": 20.0
            },
            "posterior_encoder": {
                "n_fft": 1920,
                "frame_size": 480,
                "internal_channels": 96,
                "speaker_embedding_dim": 128,
                "content_channels": 96,
                "kernel_size": 5,
                "dilation": 1,
                "num_layers": 16
            },
            "speaker_embedding": {
                "num_speakers": 8192,
                "embedding_dim": 128
            },
            "flow": {
                "content_channels": 96,
                "internal_channels": 96,
                "speaker_embedding_dim": 128,
                "kernel_size": 5,
                "dilation": 1,
                "num_layers": 4,
                "num_flows": 4
            },
            "text_encoder": {
                "num_phonemes": 512,
                "num_languages": 256,
                "lm_dim": 768,
                "internal_channels": 256,
                "speaker_embedding_dim": 128,
                "content_channels": 96,
                "n_heads": 4,
                "num_layers": 4,
                "window_size": 4
            },
            "stochastic_duration_predictor": {
                "in_channels": 96,
                "filter_channels": 256,
                "kernel_size": 5,
                "p_dropout": 0.0,
                "speaker_embedding_dim": 128
            },
            "duration_predictor": {
                "content_channels": 96,
                "internal_channels": 256,
                "speaker_embedding_dim": 128,
                "kernel_size": 7,
                "num_layers": 4
            }
        },
        "discriminator": {
            "mrd": {
                "resolutions": [
                    128,
                    256,
                    512
                ],
                "channels": 32,
                "num_layers": 6
            },
            "mpd": {
                "periods": [
                    1,
                    2,
                    3,
                    5,
                    7,
                    11
                ],
                "channels": 32,
                "channels_mul": 2,
                "max_channels": 256,
                "num_layers": 4
            }
        },
        "duration_discriminator": {
            "content_channels": 96,
            "speaker_embedding_dim": 128,
            "num_layers": 3
        },
        "optimizer": {
            "lr": 1e-4,
            "betas": [
                0.8,
                0.99
            ]
        }
    },
    "language_model": {
        "type": "rinna_roberta",
        "options": {
            "hf_repo": "rinna/japanese-roberta-base",
            "layer": 4
        }
    },
    "preprocess": {
        "sample_rate": 48000,
        "max_waveform_length": 480000,
        "pitch_estimation": "fcpe",
        "max_phonemes": 100,
        "lm_max_tokens": 30,
        "n_fft": 1920,
        "frame_size": 480,
        "cache": "dataset_cache"
    },
    "train": {
        "save": {
            "models_dir": "models",
            "interval": 400
        },
        "data_module": {
            "cache_dir": "dataset_cache",
            "metadata": "models/metadata.json",
            "batch_size": 4,
            "num_workers": 1
        },
        "trainer": {
            "devices": "auto",
            "max_epochs": 1000000,
            "precision": "bf16-mixed"
        }
    },
    "infer": {
        "n_fft": 1920,
        "frame_size": 480,
        "sample_rate": 48000,
        "max_lm_tokens": 50,
        "max_phonemes": 500,
        "max_frames": 2000,
        "device": "cuda"
    }
}